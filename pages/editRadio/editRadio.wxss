/*弹出框*/
.mock{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(00, 00, 00, 0.4)
}
.mockBg{
    width: 70%;
    height: 300rpx;
    background: #fff;
    border-radius: 20rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translateX(-50%) translateY(-50%);
    text-align: center;
}
.mockTitle{
    width: 90%;
    padding: 74rpx 0rpx;
    margin: 0 auto;
    border-bottom: 1rpx solid #eee;
}
.mockResult{
    padding-top: 30rpx;
}
.f_mock{
    display: flex;
    justify-content: space-around;
}
.f_mock view:nth-child(1){
    border-right: 1rpx solid #eee;
    padding-right: 70rpx;
    margin-top: 30rpx;
}